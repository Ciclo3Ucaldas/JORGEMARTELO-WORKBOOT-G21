@page
@model MyApp.Namespace.IndexModel

@{
    ViewData["Title"] = "List student";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table>
    <tr>
        <th>Id</th>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Fecha</th>
    </tr>
    
        @foreach (var estudiante in Model.students)
        {
    <tr>
        <td>@estudiante.ID</td>
        <td>@estudiante.LasName</td>
        <td>@estudiante.FirstMidName</td>
        <td>@estudiante.EnrollmentDate</td>
         <td>
                <a asp-action="Student/Edit" asp-route-id="@estudiante.ID">Edit</a> |
                <a asp-action="Student/Details" asp-route-id="@estudiante.ID">Details</a> |
                <a asp-action="Student/Delete" asp-route-id="@estudiante.ID">Delete</a>
            </td>
    </tr>}
</table>