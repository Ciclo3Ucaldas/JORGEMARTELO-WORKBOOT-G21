@page
@model TallerMintic.App.Frontend.Pages.ListModel
@{
}
<h1>Lista de clientes<h1>
        <table class="table table-light">
            <tbody>
                <tr>
                    <th>Id</th>
                    <th>Nombre</th>
                    <th>Cedula</th>
                    <th>Numero Telefonico</th>
                </tr>
                @foreach(var dato in Model.clientes){
                    <tr>
                        <td>@dato.Id</td>  
                        <td>@dato.Nombre</td>   
                        <td>@dato.Documento</td>
                        <td>@dato.NumeroTelefonico</td>
                    </tr>        
                }
            </tbody>
        </table>
    